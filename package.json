  },
  "homepage": "https://github.com/davidd26x/cursoCypress#readme",
  "devDependencies": {
    "allure-commandline": "^2.34.0",
    "cypress": "^14.3.3",

    "cypress-parallel": "^0.15.0",
    "parallel": "^1.3.1",

    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.1"
  }
}
